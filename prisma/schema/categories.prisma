model Categories {
  id       Int        @id @default(autoincrement())
  type     Category
  name     String
  Articles Articles[]
  watches  Watches[]
}

enum Category {
  ANALOG
  ANALOG_DIGITAL
  DIGITAL
  CHRONOGRAPH
  SMARTWATCH
  OTHER
  HYBRID
  LUXURY
  AUTOMATIC
  DIVER
  PILOT
  POCKET
  DRESS
  FIELD
  TOURBILLON
  MECHANICAL
  QUARTZ
  SOLAR
  SKELETON
  STRAPS
}
