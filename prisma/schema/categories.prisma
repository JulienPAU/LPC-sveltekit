model Categories {
  id                Int                 @id @default(autoincrement())
  type              Category
  name              String
  Articles          Articles[]
  watches           Watches[]
}

enum Category {
  ANALOG
  AUTOMATIC
  DIGITAL
  CHRONOGRAPH
  SMARTWATCH
  HYBRID
  LUXURY
  DIVER
  PILOT
  POCKET
  DRESS
  FIELD
  TOURBILLON
  MECHANICAL
  SOLAR
  SKELETON
  OTHER
}
